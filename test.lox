print min(5, 3, 7, 1);          // Output: 1
print min([5, 3, 7, 1]);        // Output: 1
print max(5, 3, 7, 1);          // Output: 7
print max([5, 3, 7, 1]);        // Output: 7
print min([3.5, -2.1, 4.8]);    // Output: -2.1
print max(10, 20, [30, 40, 5]); // Output: 40 (The array is considered as a single argument, not flattened)
